{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["DOM"],
    "module": "commonjs",
    "rootDir": "./",
    "baseUrl": "./",
    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist",
    "removeComments": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
